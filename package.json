{
  "name": "hacker-menu",
  "productName": "Hacker Menu",
  "version": "1.0.0",
  "description": "A menu to read Hacker News",
  "main": "./dist/index.js",
  "repository": {
    "type": "git",
    "url": "https://github.com/jingweno/hacker-menu.git"
  },
  "engines": {
    "node": "0.12.7",
    "npm": "2.11.3"
  },
  "bugs": {
    "url": "https://github.com/jingweno/hacker-menu/issues"
  },
  "license": "MIT",
  "config": {
    "electron_version": "0.31.2"
  },
  "scripts": {
    "build": "babel src -D -d dist",
    "csscomb": "csscomb css/layout.css",
    "watch": "babel src -D -d dist -w",
    "test": "standard",
    "postinstall": "./node_modules/.bin/electron-rebuild",
    "prestart": "npm run build",
    "start": "electron ./",
    "compress": "./script/compress.sh",
    "package": "npm run build && ./script/package.sh",
    "release": "npm run package && npm run compress"
  },
  "author": "Jingwen Owen Ou",
  "devDependencies": {
    "babel": "^5.8.21",
    "babel-eslint": "^4.0.10",
    "csscomb": "^3.1.8",
    "electron-packager": "^5.0.1",
    "electron-prebuilt": "^0.31.2",
    "electron-rebuild": "^1.0.0",
    "standard": "^5.1.0"
  },
  "dependencies": {
    "async": "^1.4.2",
    "electron-rpc": "^1.0.3",
    "firebase": "^2.2.9",
    "fs-plus": "^2.8.1",
    "lodash": "^3.10.1",
    "lru-cache": "^2.6.5",
    "menubar": "^2.2.1",
    "moment": "^2.10.6",
    "newrelic-winston": "0.0.1",
    "nslog": "^3.0.0",
    "react": "^0.13.3",
    "winston": "^1.0.1"
  }
}
