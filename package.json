{
  "name": "hacker-menu",
  "productName": "Hacker Menu",
  "version": "1.0.0",
  "description": "A menu to read Hacker News",
  "main": "./dist/index.js",
  "repository": {
    "type": "git",
    "url": "https://github.com/jingweno/hacker-menu.git"
  },
  "bugs": {
    "url": "https://github.com/jingweno/hacker-menu/issues"
  },
  "license": "MIT",
  "scripts": {
    "build": "babel src -D -d dist",
    "csscomb": "csscomb css/layout.css",
    "watch": "babel src -D -d dist -w",
    "test": "standard",
    "prestart": "npm run build",
    "start": "electron ./",
    "package": "npm run build && electron-packager . 'Hacker Menu' --overwrite --sign='Developer ID Application: Jingwen Ou' --icon='images/Icon@1024.icns' --platform=darwin --arch=x64 --version=0.31.1 --ignore='src' --ignore='node_modules/(babel|standard)' --ignore='node_modules/electron-(packager|prebuild|rebuild)'"
  },
  "author": "Jingwen Owen Ou",
  "license": "ISC",
  "devDependencies": {
    "babel": "^5.8.21",
    "babel-eslint": "^4.0.10",
    "csscomb": "^3.1.8",
    "electron-packager": "^5.0.1",
    "electron-prebuilt": "^0.31.1",
    "standard": "^5.1.0"
  },
  "dependencies": {
    "async": "^1.4.2",
    "electron-rpc": "^1.0.3",
    "firebase": "^2.2.9",
    "fs-plus": "^2.8.1",
    "lru-cache": "^2.6.5",
    "menubar": "^2.1.2",
    "moment": "^2.10.6",
    "react": "^0.13.3",
    "underscore": "^1.8.3",
    "winston": "^1.0.1"
  }
}
